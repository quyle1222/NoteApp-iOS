{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "types": ["react-native", "jest", "node"],
    "typeRoots": [
      "@types",
      "./node_modules/@types"
    ],
    "jsx": "react",
    "target": "es6",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "types/*": ["./@types/*"]
    },
    "moduleResolution":"node",
  }
}
